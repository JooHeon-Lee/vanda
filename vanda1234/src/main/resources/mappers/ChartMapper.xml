<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.vanda.mapper.ChartMapper">

	<select id ="getChart" resultType="WeightVO">
		select * from Weight where pet_num = #{pet_num}
	</select>
	
	<select id ="getActivity" resultType="ActivityVO">
		select * from Activity where pet_num = #{pet_num}
	</select>
	
	
</mapper>	